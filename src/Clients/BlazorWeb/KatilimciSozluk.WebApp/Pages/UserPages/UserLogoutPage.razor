@page "/logout"

@code {
    [Inject]
    IIdentityService identityService { get; set; }

    [Inject]
    NavigationManager navigationManager { get; set; }

    protected override void OnInitialized()
    {
        identityService.Logout();
        navigationManager.NavigateTo("/");
    }

